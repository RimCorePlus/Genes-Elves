<Defs>
    <RulePackDef>
        <defName>RCP_NamerGoblin</defName>
        <rulePack>
            <rulesFiles>
                <li>EndGoblin->WordParts/End_Goblin</li>
                <li>SylGoblin->WordParts/Syllables_Goblin</li>
                <li>nicknameGoblin->Names/Nick_Unisex_Goblin</li>
            </rulesFiles>
        </rulePack>
    </RulePackDef>
    <RulePackDef>
        <defName>RCP_NamerPersonGoblin</defName>
        <include>
            <li>RCP_NamerGoblin</li>
        </include>
        <rulePack>
            <rulesStrings>
                <li>r_name->[firstName] [lastName]</li>
                <li>r_name->[firstName] '[nicknameGoblin]' [lastName]</li>
                <li>firstName->[SylGoblin][SylGoblin]</li>
                <li>firstName->[SylGoblin]</li>
                <li>lastName->[SylGoblin][EndGoblin]</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>
    <RulePackDef>
        <defName>RCP_NamerFactionGoblin</defName>
        <include>
            <li>RCP_NamerGoblin</li>
        </include>
        <rulePack>
            <rulesStrings>
                <li>r_name->[SylGoblin][SylGoblin][EndGoblin]</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>
    <RulePackDef>
        <defName>RCP_NamerSettlementGoblin</defName>
        <include>
            <li>RCP_NamerGoblin</li>
        </include>
        <rulePack>
            <rulesFiles>
                <li>TerrainFeature->Words/Nouns/TerrainFeatures</li>
            </rulesFiles>
            <rulesStrings>
                <li>r_name->[lastName]</li>
                <li>r_name->[firstName]'s [TerrainFeature]</li>
                <li>r_name->[firstName]'s [GoblinFortification]</li>
                <li>firstName->[SylGoblin][SylGoblin]</li>
                <li>firstName->[SylGoblin]</li>
                <li>lastName->[SylGoblin][EndGoblin]</li>
                <li>GoblinFortification->barricade</li>
                <li>GoblinFortification->burrow</li>
                <li>GoblinFortification->camp</li>
                <li>GoblinFortification->den</li>
                <li>GoblinFortification->fanghall</li>
                <li>GoblinFortification->hideout</li>
                <li>GoblinFortification->hovel</li>
                <li>GoblinFortification->lair</li>
                <li>GoblinFortification->mudhut</li>
                <li>GoblinFortification->outpost</li>
                <li>GoblinFortification->pit</li>
                <li>GoblinFortification->rubble</li>
                <li>GoblinFortification->scrapfort</li>
                <li>GoblinFortification->shanty</li>
                <li>GoblinFortification->slag</li>
                <li>GoblinFortification->spikewall</li>
                <li>GoblinFortification->stockade</li>
                <li>GoblinFortification->stronghold</li>
                <li>GoblinFortification->trashfort</li>
                <li>GoblinFortification->warren</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>
</Defs>