<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Medieval Overhaul</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>//FactionDef[starts-with(defName, "DankPyon_NobleHouse")]/xenotypeSet/xenotypeChances</xpath>
                    <value>
                        <CorePlus_DarkElf>0.025</CorePlus_DarkElf>
                        <CorePlus_HighElf>0.05</CorePlus_HighElf>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>